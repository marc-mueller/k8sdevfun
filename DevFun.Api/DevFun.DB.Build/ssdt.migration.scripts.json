{
  "PreScripts": {
    "ScriptBaseDirectory": "../DevFun.DB/Scripts/PreScripts",
    "ScriptNamePattern": "^(\\d{14})_(.*).sql",
    "ScriptRecursiveSearch": true,
    "ExecutionFilterMode": "all",
    "ExecutionFilterValue": "",

    "TreatScriptNamePatternMismatchAsError": true,
    "TreatHashMismatchAsError": true
  },

  "PostScripts": {
    "ScriptBaseDirectory": "../DevFun.DB/Scripts/PostScripts",
    "ScriptNamePattern": "^(\\d{14})_(.*).sql",
    "ScriptRecursiveSearch": true,
    "ExecutionFilterMode": "all",
    "ExecutionFilterValue": "",

    "TreatScriptNamePatternMismatchAsError": true,
    "TreatHashMismatchAsError": true
  },

  "ReferenceDataScripts": {
    "ScriptBaseDirectory": "../DevFun.DB/Scripts/ReferenceDataScripts",
    "ScriptNamePattern": "^(\\d+)_(.*).sql",
    "ScriptRecursiveSearch": true,
    "ExecutionFilterMode": "all",
    "ExecutionFilterValue": "",

    "TreatScriptNamePatternMismatchAsError": true,
    "TreatHashMismatchAsError": true
  }
}